{"components/category/shortcut-categories":"{{#if breadcrumbs.[1].name '===' 'Men'}}\r\n<div class=\"pl-filter-wrap\">\r\n    <label for=\"sort\"><img src=\"/content/images/common/filter.svg\"></label>\r\n    <select name=\"filter\" id=\"pl-filter\">\r\n        <option value=\"\" selected disabled>Filter By</option>\r\n        <option value=\"/men-hair-pieces-toupees-in-stock/\" {{#contains category.url '/men-hair-pieces-toupees-in-stock/'}}selected{{/contains}}>All Items</option>\r\n        <option value=\"/silk-top-mens-toupee-1/\" {{#contains category.url '/silk-top-mens-toupee-1/'}}selected{{/contains}}>Base | Mono</option>\r\n        <option value=\"/full-lace-base-mens-toupee-1/\" {{#contains category.url '/full-lace-base-mens-toupee-1/'}}selected{{/contains}}>Base | Lace</option>\r\n        <option value=\"/poly-skin-mens-toupee-2/\" {{#contains category.url '/poly-skin-mens-toupee-2/'}}selected{{/contains}}>Base | Skin</option>\r\n        <option value=\"/lace-front-mens-toupee-1/\" {{#contains category.url '/lace-front-mens-toupee-1/'}}selected{{/contains}}>Base | Lace Front</option>\r\n        <option value=\"/small-size-6x-8-or-6-5x-8-5-1/\" {{#contains category.url '/small-size-6x-8-or-6-5x-8-5-1/'}}selected{{/contains}}>Size | Small</option>\r\n        <option value=\"/medium-size-7x-9-1/\" {{#contains category.url '/medium-size-7x-9-1/'}}selected{{/contains}}>Size | Medium</option>\r\n        <option value=\"/large-size-about-8-x-10-2/\" {{#contains category.url '/large-size-about-8-x-10-2/'}}selected{{/contains}}>Size | Large</option>\r\n        <option value=\"/light-density-90-1/\" {{#contains category.url '/light-density-90-1/'}}selected{{/contains}}>Density | Light</option>\r\n        <option value=\"/light-to-medium-110-2/\" {{#contains category.url '/light-to-medium-110-2/'}}selected{{/contains}}>Density | Light to Medium</option>\r\n        <option value=\"/medium-120-1/\" {{#contains category.url '/medium-120-1/'}}selected{{/contains}}>Density