{"components/common/alert-error":"<div class=\"alertBox alertBox--error\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n","components/common/alert-success":"<div class=\"alertBox alertBox--success\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>\r\n","components/products/add-to-cart":"<div class=\"form-field form-field--increments\">\r\n    <label class=\"form-label form-label--alternate\"\r\n           for=\"qty[]\">{{lang 'products.quantity'}}</label>\r\n\r\n    <div class=\"form-increment\" data-quantity-change>\r\n        <button class=\"button button--icon\" data-action=\"dec\">\r\n            <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n            <i class=\"icon\" aria-hidden=\"true\">\r\n                <svg>\r\n                    <use xlink:href=\"#icon-keyboard-arrow-down\"/>\r\n                </svg>\r\n            </i>\r\n        </button>\r\n        <input class=\"form-input form-input--incrementTotal\"\r\n               id=\"qty[]\"\r\n               name=\"qty[]\"\r\n               type=\"tel\"\r\n               value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\r\n               data-quantity-min=\"{{product.min_purchase_quantity}}\"\r\n               data-quantity-max=\"{{product.max_purchase_quantity}}\"\r\n               min=\"1\"\r\n               pattern=\"[0-9]*\"\r\n               aria-live=\"polite\">\r\n        <button class=\"button button--icon\" data-action=\"inc\">\r\n            <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\r\n            <i class=\"icon\" aria-hidden=\"true\">\r\n                <svg>\r\n                    <use xlink:href=\"#icon-keyboard-arrow-up\"/>\r\n                </svg>\r\n            </i>\r\n        </button>\r\n    </div>\r\n</div>\r\n\r\n<div class=\"alertBox productAttributes-message\" style=\"display:none\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\"></p>\r\n</div>\r\n{{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n    <div class=\"form-action\">\r\n        <input id=\"form-action-addToCart\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button button--primary\" type=\"submit\"\r\n            value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\">\r\n    </div>\r\n     {{{snippet 'product_addtocart'}}}\r\n{{/or}}\r\n","components/products/customizations/checkbox":"<div class=\"form-field\">\r\n    <input class=\"form-checkbox\" type=\"checkbox\" id=\"customization_{{this.id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{this.id}}]\" {{#if value}}checked{{/if}}>\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{this.id}}_{{../id}}\">\r\n        {{ this.display_name }}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n</div>\r\n","components/products/customizations/file":"<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{this.id}}_{{../id}}\">\r\n        {{ this.display_name }}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <input class=\"form-file\" type=\"file\" id=\"customization_{{this.id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{this.id}}]\">\r\n    {{#if original_name}}\r\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\r\n    {{/if}}\r\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\r\n</div>\r\n","components/products/customizations/select":"<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\r\n        {{ display_name }}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <select class=\"form-select form-select--small\" name=\"ProductFields[{{id}}]\" id=\"customization_{{id}}_{{../id}}\"{{#if required}} required{{/if}}>\r\n        <option value=\"\">{{lang 'products.choose_an_option'}}</option>\r\n        {{#each values}}\r\n            <option value=\"{{this}}\" {{#if ../value '===' this}}selected{{/if}}>{{this}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n","components/products/customizations/text":"<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\r\n        {{ display_name }}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"customization_{{id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{id}}]\" value=\"{{value}}\">\r\n</div>\r\n","components/products/customizations/textarea":"<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\r\n        {{display_name}}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <textarea class=\"form-input\" id=\"customization_{{id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{id}}]\">{{value}}</textarea>\r\n</div>\r\n","components/products/options/date":"<div class=\"form-field\" data-product-attribute=\"date\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\r\n        {{this.display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.month'}}</option>\r\n        {{#for 1 12}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{lang (concat 'common.short_months.' $index)}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.day'}}</option>\r\n        {{#for 1 31}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{$index}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.year'}}</option>\r\n        {{#for earliest_year latest_year}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{$index}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n</div>\r\n","components/products/options/input-checkbox":"<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input \r\n        class=\"form-checkbox\"\r\n        type=\"checkbox\"\r\n        name=\"attribute[{{id}}]\"\r\n        id=\"attribute-{{id}}\"\r\n        value=\"{{value}}\"\r\n        {{#if checked}}checked{{/if}}\r\n        {{#if required}}required{{/if}}>\r\n\r\n    <label class=\"form-label {{class}}\" for=\"attribute-{{id}}\">{{label}}</label>\r\n</div>\r\n","components/products/options/input-file":"<div class=\"form-field\" data-product-attribute=\"input-file\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input class=\"form-file\" type=\"file\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\r\n    {{#if original_name}}\r\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\r\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\r\n    {{/if}}\r\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\r\n\r\n</div>\r\n","components/products/options/input-numbers":"<div class=\"form-field\" data-product-attribute=\"input-number\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input class=\"form-input\" type=\"number\" id=\"attribute_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n","components/products/options/input-text":"<div class=\"form-field\" data-product-attribute=\"input-text\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n","components/products/options/product-list":"<div class=\"form-field\" data-product-attribute=\"product-list\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{display_name}}:\r\n        <span data-option-value></span>\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    {{#if values.0.image}}\r\n        <ul class=\"productOptions-list\">\r\n            {{#unless required}}\r\n                <li class=\"productOptions-list-item\">\r\n                    <input class=\"form-radio\"\r\n                           type=\"radio\"\r\n                           name=\"attribute[{{id}}]\"\r\n                           value=\"0\"\r\n                           id=\"attribute_0_{{id}}\"\r\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\r\n                    <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\r\n                </li>\r\n            {{/unless}}\r\n            {{#each values}}\r\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\r\n                    {{#if image}}\r\n                        <figure class=\"productOptions-list-item-figure\">\r\n                            <img class=\"productOptions-list-item-image\" src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                        </figure>\r\n                    {{/if}}\r\n                    <div class=\"productOptions-list-item-content\">\r\n                        <input\r\n                            class=\"form-radio\"\r\n                            type=\"radio\"\r\n                            name=\"attribute[{{../id}}]\"\r\n                            value=\"{{id}}\"\r\n                            id=\"attribute_{{id}}\"\r\n                            {{#if selected}}checked{{/if}}\r\n                            {{#if ../required}}required{{/if}}>\r\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    {{else}}\r\n        {{#unless required}}\r\n        <input class=\"form-radio\"\r\n               type=\"radio\"\r\n               name=\"attribute[{{id}}]\"\r\n               value=\"0\"\r\n               id=\"attribute_0_{{id}}\"\r\n               {{#if defaultValue '==' 0}}checked{{/if}}>\r\n        <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\r\n        {{/unless}}\r\n        {{#each values}}\r\n            <input\r\n                class=\"form-radio\"\r\n                type=\"radio\"\r\n                name=\"attribute[{{../id}}]\"\r\n                value=\"{{id}}\"\r\n                id=\"attribute_{{id}}\"\r\n                {{#if selected}}checked{{/if}}\r\n                {{#if ../required}}required{{/if}}>\r\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n","components/products/options/set-radio":"<div class=\"form-field\" data-product-attribute=\"set-radio\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}checked{{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{this.label}}</label>\r\n    {{/each}}\r\n</div>\r\n","components/products/options/set-rectangle":"<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{this.display_name}}:\r\n        <span data-option-value></span>\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}checked{{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label class=\"form-option\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n            <span class=\"form-option-variant\">{{this.label}}</span>\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n","components/products/options/set-select":"<div class=\"form-field\" data-product-attribute=\"set-select\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_{{id}}\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\r\n        {{#each this.values}}\r\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected{{/if}}>{{label}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n","components/products/options/swatch":"<div class=\"form-field\" data-product-attribute=\"swatch\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{this.display_name}}:\r\n        <span data-option-value></span>\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_{{id}}\" {{#if selected}}checked{{/if}} {{#if ../required}}required{{/if}}>\r\n        <label class=\"form-option form-option-swatch\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n            {{#if image}}\r\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\"></span>\r\n            {{/if}}\r\n            {{#if data.[2]}}\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\r\n            {{else}}\r\n                {{#if data.[1]}}\r\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n                {{else}}\r\n                    {{#if data.[0]}}\r\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/if}}\r\n            {{#if pattern}}\r\n                <span class=\"form-option-expanded\">\r\n                    <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\r\n                </span>\r\n            {{/if}}\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n","components/products/options/textarea":"<div class=\"form-field\" data-product-attribute=\"textarea\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <textarea class=\"form-input\" id=\"attribute_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>\r\n        {{prefill}}\r\n    </textarea>\r\n</div>\r\n","components/products/preorder-product-view":"<div class=\"productView\">\r\n    {{#each product.reviews.messages}}\r\n        {{#if error}}\r\n            {{> components/common/alert-error error}}\r\n        {{/if}}\r\n        {{#if success}}\r\n            {{> components/common/alert-success success}}\r\n        {{/if}}\r\n    {{/each}}\r\n\r\n    <section class=\"productView-details\">\r\n        <div class=\"productView-product\">\r\n            <h1 class=\"productView-title\" {{#if schema}}itemprop=\"name\"{{/if}}>{{lang product.title}}</h1>\r\n\r\n            <h2 class=\"productView-brand\"{{#if schema}} itemprop=\"brand\" itemscope itemtype=\"http://schema.org/Brand\"{{/if}}>\r\n                <a href=\"{{product.brand.url}}\"{{#if schema}} itemprop=\"url\"{{/if}}><span{{#if schema}} itemprop=\"name\"{{/if}}>{{product.brand.name}}</span></a>\r\n            </h2>\r\n            {{#if product.call_for_price}}\r\n                <p class=\"productView-price\">\r\n                    <span>{{product.call_for_price}}</span>\r\n                </p>\r\n            {{/if}}\r\n            <div class=\"productView-price\">\r\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                    {{> components/products/price price=product.price schema_org=schema}}\r\n                {{else}}\r\n                    {{> components/common/login-for-pricing}}\r\n                {{/or}}\r\n            </div>\r\n            {{{region name=\"product_below_price\"}}}            \r\n            {{product.detail_messages}}\r\n            <dl class=\"productView-info\">                                              \r\n                {{#if product.min_purchase_quantity}}\r\n                    <dt class=\"productView-info-name\">{{lang 'products.min_purchase_quantity'}}</dt>\r\n                    <dd class=\"productView-info-value\">{{lang 'products.purchase_units' quantity=product.min_purchase_quantity}}</dd>\r\n                {{/if}}\r\n                {{#if product.max_purchase_quantity}}\r\n                    <dt class=\"productView-info-name\">{{lang 'products.max_purchase_quantity'}}</dt>\r\n                    <dd class=\"productView-info-value\">{{lang 'products.purchase_units' quantity=product.max_purchase_quantity}}</dd>\r\n                {{/if}}\r\n                {{#if product.gift_wrapping_available}}\r\n                    <dt class=\"productView-info-name\">{{lang 'products.gift_wrapping'}}</dt>\r\n                    <dd class=\"productView-info-value\">{{lang 'products.gift_wrapping_available'}}</dd>\r\n                {{/if}}                \r\n                {{#if product.bulk_discount_rates.length}}\r\n                    <dt class=\"productView-info-name\">{{lang 'products.bulk_pricing.title'}}</dt>\r\n                    <dd class=\"productView-info-value\">\r\n                        <a href=\"{{product.url}}\"\r\n                            {{#unless is_ajax }}data-reveal-id=\"bulkPricingModal\" {{/unless}}>\r\n                            {{lang 'products.bulk_pricing.view'}}\r\n                        </a>\r\n                    </dd>\r\n\r\n                    <div id=\"bulkPricingModal\" class=\"modal modal--small\" data-reveal>\r\n                        <div class=\"modal-header\">\r\n                            <h2 class=\"modal-header-title\">{{lang 'products.bulk_pricing.modal_title'}}</h2>\r\n                            <a href=\"#\" class=\"modal-close\" aria-label=\"Close\"><span aria-hidden=\"true\">&#215;</span></a>\r\n                        </div>\r\n                        <div class=\"modal-body\">\r\n                            <p>{{lang 'products.bulk_pricing.instructions'}}</p>\r\n                            <ul>\r\n                            {{#each product.bulk_discount_rates}}\r\n                                <li>\r\n                                    {{lang 'products.bulk_pricing.range' min=min max=max}}\r\n                                    {{#if type '===' 'percent'}}\r\n                                        {{lang 'products.bulk_pricing.percent' discount=discount.formatted}}\r\n                                    {{/if}}\r\n                                    {{#if type '===' 'fixed'}}\r\n                                        {{lang 'products.bulk_pricing.fixed' discount=discount.formatted}}\r\n                                    {{/if}}\r\n                                    {{#if type '===' 'price'}}\r\n                                        {{lang 'products.bulk_pricing.price' discount=discount.formatted}}\r\n                                    {{/if}}\r\n                                </li>\r\n                            {{/each}}\r\n                            </ul>\r\n                        </div>\r\n                    </div>\r\n                {{/if}}\r\n\r\n                {{#each product.custom_fields}}\r\n                    <dt class=\"productView-info-name\">{{name}}:</dt>\r\n                    <dd class=\"productView-info-value\">{{{value}}}</dd>\r\n                {{/each}}\r\n            </dl>\r\n        </div>\r\n    </section>\r\n\r\n    <section class=\"productView-images\" data-image-gallery>\r\n        <figure class=\"productView-image\"\r\n                data-image-gallery-main\r\n                data-zoom-image=\"{{getImage product.main_image 'zoom_size' (cdn theme_settings.default_image_product)}}\"\r\n                >\r\n            <a href=\"{{getImage product.main_image 'zoom_size' (cdn theme_settings.default_image_product)}}\">\r\n                <img class=\"productView-image--default\"\r\n                     src=\"{{getImage product.main_image 'product_size' (cdn theme_settings.default_image_product)}}\"\r\n                     alt=\"{{product.main_image.alt}}\" title=\"{{product.main_image.alt}}\" data-main-image>\r\n            </a>\r\n        </figure>\r\n        <ul class=\"productView-thumbnails\"{{#gt product.images.length 5}} data-slick='{\r\n                \"infinite\": false,\r\n                \"mobileFirst\": true,\r\n                \"slidesToShow\": 5,\r\n                \"slidesToScroll\": 1\r\n            }'{{/gt}}>\r\n            {{#each product.images}}\r\n                <li class=\"productView-thumbnail\">\r\n                    <a\r\n                        class=\"productView-thumbnail-link\"\r\n                        href=\"{{getImage this 'product_size' (cdn ../theme_settings.default_image_product)}}\"\r\n                        data-image-gallery-item\r\n                        data-image-gallery-new-image-url=\"{{getImage this 'product_size' (cdn ../theme_settings.default_image_product)}}\"\r\n                        data-image-gallery-zoom-image-url=\"{{getImage this 'zoom_size' (cdn ../theme_settings.default_image_product)}}\">\r\n                        <img src=\"{{getImage this 'productthumb_size' (cdn ../theme_settings.default_image_product)}}\" alt=\"{{this.alt}}\" title=\"{{this.alt}}\">\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    </section>\r\n\r\n    <section class=\"productView-details\">\r\n        <div class=\"productView-options pre-order-options\">\r\n            <form class=\"form\" method=\"post\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\"\r\n                  data-cart-item-add>\r\n                <input type=\"hidden\" name=\"action\" value=\"add\">\r\n                <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\r\n                    {{#each product.customizations}}\r\n                        {{{dynamicComponent 'components/products/customizations'}}}\r\n                    {{/each}}\r\n                <div data-product-option-change>\r\n                    {{inject 'product_options' product.options}}\r\n                    <script>\r\n                        var jsContext = JSON.parse({{jsContext}});\r\n                        console.log(jsContext);\r\n                    </script>\r\n                    {{#each product.options}}\r\n                        {{{dynamicComponent 'components/products/options'}}}\r\n                    {{/each}}\r\n                </div>                \r\n                {{> components/products/add-to-cart}}                \r\n            </form>            \r\n        </div>        \r\n        {{{snippet 'product_details'}}}\r\n    </section>    \r\n</div>\r\n\r\n<div id=\"previewModal\" class=\"modal modal--large\" data-reveal>\r\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n        <span aria-hidden=\"true\">&#215;</span>\r\n    </a>\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n<script>\r\n    let sels=document.querySelector(\".pre-order-options\").querySelectorAll(\".form-field\");sels.forEach(e=>{let t=(e.querySelector(\".form-label\").innerText||e.querySelector(\".form-label\").textContent).toLowerCase();if(0==t.includes(\"choose base\")&&0==t.includes(\"hair color\")&&0==t.includes(\"grey hair\")&&0==t.includes(\"quantity\")&&0==t.includes(\"wigs\"))e.getAttribute(\"data-product-attribute\")&&\"input-checkbox\"==e.getAttribute(\"data-product-attribute\")||(e.style.display=\"none\");else if(t.includes(\"choose base\")){document.querySelector(\".product-sku\")&&document.querySelector(\".product-sku\").innerHTML;let t=(document.querySelector(\".product-name\")?document.querySelector(\".product-name\").innerHTML:\"\").split(\" \"),r=0,l=0;Array.from(e.querySelector(\"select\").options).forEach((e,o)=>{let c=e.text.split(\" \"),s=0;c.forEach(e=>{t.includes(e)&&s++}),s>l&&(l=s,r=o)}),e.querySelector(\"select\").selectedIndex=r}else t.includes(\"color\")||t.includes(\"wigs\")?e.classList.add(\"select-color\"):t.includes(\"grey\")&&(e.style.display=\"none\",e.classList.add(\"select-grey\"))});   \r\n</script>\r\n","components/products/price":"{{#or has_options product.options.length}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n                <meta itemprop=\"availability\" content=\"{{product.availability}}\">\r\n                <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{else}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{/or}}\r\n"}
