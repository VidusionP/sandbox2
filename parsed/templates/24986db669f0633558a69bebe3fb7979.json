{"components/common/forms/radio":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required' }}</small>\r\n        {{/if}}\r\n    </label>\r\n    {{#if radios}}\r\n        {{#each radios}}\r\n            <input \r\n                type=\"radio\"\r\n                class=\"form-radio\"\r\n                name=\"{{../name}}\"\r\n                id=\"{{../name}}_{{@index}}\"\r\n                value=\"{{value}}\"\r\n                data-label=\"{{label}}\"\r\n                aria-required=\"{{required}}\"\r\n                {{#if checked}}\r\n                    checked\r\n                {{/if}}\r\n                {{#if private_id}}\r\n                    data-field-type=\"{{private_id}}\"\r\n                {{/if}}>\r\n            <label class=\"form-label\" for=\"{{../name}}_{{@index}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n"}
