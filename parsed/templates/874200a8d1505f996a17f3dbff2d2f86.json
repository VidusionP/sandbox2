{"components/cart/coupon-input":"<div class=\"cart-total-value\">\r\n\r\n    <button class=\"coupon-code-add\">{{lang 'cart.coupons.add_coupon'}}</button>\r\n\r\n    <button class=\"coupon-code-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\r\n</div>\r\n\r\n<div class=\"cart-form coupon-code\" style=\"display: none;\">\r\n    <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"{{urls.cart}}\">\r\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.coupons.coupon_code'}}</label>\r\n        <input class=\"form-input\" data-error=\"{{lang 'cart.coupons.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupons.coupon_code'}}\">\r\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\r\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\r\n    </form>\r\n</div>\r\n","components/cart/gift-certificate-input":"<div class=\"cart-total-value\">\r\n\r\n    <button class=\"gift-certificate-add\">{{lang 'cart.gift_certificates.gift_certificate'}}</button>\r\n\r\n    <button class=\"gift-certificate-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\r\n</div>\r\n\r\n<div class=\"cart-form gift-certificate-code\" style=\"display: none;\">\r\n    <form class=\"form form--hiddenLabels cart-gift-certificate-form\" method=\"post\" action=\"{{urls.cart}}\">\r\n        <label class=\"form-label\" for=\"certcode\">{{lang 'cart.gift_certificates.cert_code'}}</label>\r\n        <input class=\"form-input\" data-error=\"{{lang 'cart.gift_certificates.empty_error'}}\"  id=\"certcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.add_cert_code'}}\">\r\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\r\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\r\n    </form>\r\n</div>\r\n","components/cart/shipping-estimator":"{{#if shipping_cost}}\r\n    <div class=\"cart-total-value\">\r\n        <div class=\"subtotal shipping-estimate-show\">\r\n            <a href=\"#\" class=\"shipping-estimate-value\">{{shipping_cost.formatted}}</a>\r\n        </div>\r\n    </div>\r\n{{else}}\r\n    <div class=\"cart-total-value\">\r\n        <button class=\"shipping-estimate-show\">{{lang 'cart.shipping_estimator.add_info'}}</button>\r\n        <button class=\"shipping-estimate-hide\" style=\"display: none;\">Cancel</button>\r\n    </div>\r\n{{/if}}\r\n\r\n<div class=\"shipping-estimator u-hiddenVisually\">\r\n    <form class=\"form estimator-form\" data-shipping-estimator>\r\n        <dl>\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input\">\r\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\r\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\r\n                    {{#each countries}}\r\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                            {{name}}\r\n                        </option>\r\n                    {{/each}}\r\n                </select>\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input\">\r\n                {{#if states.length}}\r\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\r\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\r\n                        {{#each states}}\r\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                                {{name}}\r\n                            </option>\r\n                        {{/each}}\r\n                    </select>\r\n                {{else}}\r\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\r\n                {{/if}}\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\r\n            </dd>\r\n            <button class=\"button button--primary button--small shipping-estimate-submit\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\r\n        </dl>\r\n    </form>\r\n    <div class=\"shipping-quotes\"></div>\r\n</div>\r\n","components/cart/totals":"<ul class=\"cart-totals\">\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.checkout.subtotal'}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                <span>{{cart.sub_total.formatted}}</span>\r\n            {{else}}\r\n                {{> components/common/login-for-pricing}}\r\n            {{/or}}\r\n        </div>\r\n    </li>\r\n    {{#if cart.gift_wrapping_cost.amount}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.gift_wrapping'}}:</strong>\r\n            </div>\r\n            <div class=\"cart-total-value\">\r\n                <span>{{cart.gift_wrapping_cost.formatted}}</span>\r\n            </div>\r\n        </li>\r\n    {{/if}}\r\n    {{#if cart.shipping_handling.show_estimator}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.shipping'}}:</strong>\r\n            </div>\r\n            {{> components/cart/shipping-estimator cart.shipping_handling}}\r\n        </li>\r\n    {{/if}}\r\n    {{#each cart.taxes}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{name}}:</strong>\r\n            </div>\r\n            <div class=\"cart-total-value\">\r\n                <span>{{cost.formatted}}</span>\r\n            </div>\r\n        </li>\r\n    {{/each}}\r\n    {{#if cart.discount }}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.discount'}}:</strong>\r\n            </div>\r\n            <div class=\"cart-total-value\">\r\n                {{cart.discount.formatted}}\r\n            </div>\r\n        </li>\r\n    {{/if}}\r\n    {{#if cart.coupons}}\r\n        {{#each cart.coupons}}\r\n            <li class=\"cart-total\">\r\n                <div class=\"cart-total-label\">\r\n                    <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\r\n                    <div>\r\n                        <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\r\n                    </div>\r\n                </div>\r\n                <div class=\"cart-total-value\">\r\n                    <span>{{discount.formatted}}</span>\r\n                </div>\r\n            </li>\r\n        {{/each}}\r\n    {{else}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.coupon_code'}}:</strong>\r\n            </div>\r\n            {{> components/cart/coupon-input}}\r\n        </li>\r\n    {{/if}}\r\n    {{#if settings.gift_certificates_enabled}}\r\n        {{#each cart.gift_certificates}}\r\n            <li class=\"cart-total\">\r\n                <div class=\"cart-total-label\">\r\n                    <strong>{{lang 'cart.gift_certificates.code_label' code=code}}</strong>\r\n                    <span>\r\n                        {{remaining.formatted}} | <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\r\n                    </span>\r\n                </div>\r\n                <div class=\"cart-total-value\">\r\n                    {{used.formatted}}\r\n                </div>\r\n            </li>\r\n        {{/each}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.gift_certificates.gift_certificate'}}:</strong>\r\n            </div>\r\n            {{> components/cart/gift-certificate-input}}\r\n        </li>\r\n    {{/if}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.checkout.grand_total'}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value cart-total-grandTotal\">\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                <span>{{cart.grand_total.formatted}}</span>\r\n            {{else}}\r\n                {{> components/common/login-for-pricing}}\r\n            {{/or}}\r\n        </div>\r\n    </li>\r\n</ul>\r\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>\r\n"}
