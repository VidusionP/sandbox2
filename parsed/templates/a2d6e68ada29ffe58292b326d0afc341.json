{"components/category/subcategories":"{{#if category.subcategories}}\r\n<div class=\"sct-mobile-wrap {{#if (length category.subcategories) '>' 9}}overflow{{/if}}\">\r\n    <span class=\"top\">{{category.name}}</span>\r\n    <div class=\"sct-mobile-content-wrap\">\r\n        {{#each category.subcategories}}                \r\n            <a href=\"{{url}}\">{{name}}</a>                    \r\n        {{/each}}\r\n    </div>\r\n    <span action=\"close\"><img src=\"/content/images/common/close.svg\" alt=\"\"></span>\r\n</div>\r\n{{/if}}\n"}
